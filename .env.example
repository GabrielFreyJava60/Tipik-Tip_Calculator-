# EAS Project Configuration
# Copy this file to .env and fill in your values

# Expo EAS Project ID (get from: eas build:configure)
EAS_PROJECT_ID=your-project-id-here

# Apple Developer Information
APPLE_ID=your.email@example.com
APPLE_TEAM_ID=YOUR_TEAM_ID
ASC_APP_ID=YOUR_ASC_APP_ID

# App Information
BUNDLE_IDENTIFIER=com.gabrielfrey.liquidtipcalculator
